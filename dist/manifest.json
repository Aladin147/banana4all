{
  "id": "com.banana4all.pro",
  "name": "Banana4All Pro",
  "version": "1.0.0",
  "main": "index.html",
  "host": [
    {
      "app": "PS",
      "minVersion": "23.0.0"
    }
  ],
  "manifestVersion": 5,
  "entrypoints": [
    {
      "type": "panel",
      "id": "banana4all.pro.panel",
      "label": {
        "default": "Banana4All Pro"
      },
      "minimumSize": { "width": 230, "height": 200 },
      "maximumSize": { "width": 2000, "height": 2000 },
      "preferredDockedSize": { "width": 230, "height": 300 },
      "preferredFloatingSize": { "width": 400, "height": 300 },
      "icons": [
        { "width": 23, "height": 23, "path": "icons/icon-dark.svg", "scale": [1, 2], "theme": ["darkest", "dark", "medium"] },
        { "width": 23, "height": 23, "path": "icons/icon-light.svg", "scale": [1, 2], "theme": ["lightest", "light"] }
      ]
    }
  ],
  "requiredPermissions": {
    "network": {
      "domains": [
        "http://localhost:3000"
      ]
    },
    "clipboard": "readAndWrite",
    "localFileSystem": "fullAccess"
  },
  "icons": [
    { "width": 48, "height": 48, "path": "icons/icon-dark.svg", "scale": [1, 2], "theme": ["darkest", "dark", "medium", "all"], "species": ["pluginList"] },
    { "width": 48, "height": 48, "path": "icons/icon-light.svg", "scale": [1, 2], "theme": ["lightest", "light", "all"], "species": ["pluginList"] }
  ]
}